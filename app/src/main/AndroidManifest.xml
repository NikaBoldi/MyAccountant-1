<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="automation.test.myaccoutant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity" />
        <activity
            android:name=".view.LogIn"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".view.SignUp" />
        <activity android:name=".view.SignUpForm" />
        <activity
            android:name=".view.Loading"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".view.Tab1" />
        <activity android:name=".view.Tab2" />
        <activity android:name=".view.Tab3" />
        <activity android:name=".view.SettingsActivity" />
        <activity
            android:name=".view.AboutActivity"
            android:label="@string/about" />
        <activity android:name=".view.NotifSetActivity"></activity>
    </application>

</manifest>